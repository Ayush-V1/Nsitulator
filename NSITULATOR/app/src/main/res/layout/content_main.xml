        android:layout_alignStart="@+id/tv9"
        android:gravity="center_vertical|center"
        android:textSize="20dp"
        android:textColor="#01b29b" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="%"
        android:id="@+id/textView11"
        android:gravity="center|fill_vertical"
        android:textSize="30dp"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/per"
        android:layout_alignRight="@+id/eleven"
        android:layout_alignEnd="@+id/eleven"
        android:textColor="#008978" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/twelve"
        android:layout_below="@+id/five"
        android:layout_alignRight="@+id/five"
        android:layout_alignEnd="@+id/five"
        android:textSize="15dp"
        android:phoneNumber="true"
        android:gravity="bottom|center"
        android:layout_alignLeft="@+id/per"
        android:layout_alignStart="@+id/per" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/eleven"
        android:layout_alignBottom="@+id/twelve"
        android:layout_alignParentRight="true"
