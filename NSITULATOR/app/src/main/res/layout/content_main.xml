    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="VS 2"
        android:id="@+id/tv12"
        android:layout_below="@+id/five"
        android:layout_toLeftOf="@+id/five"
        android:layout_alignBottom="@+id/twelve"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="20dp"
        android:gravity="center_vertical|center"
        android:textColor="#01b29b" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="VS 1"
        android:id="@+id/tv11"
        android:textSize="20dp"
        android:gravity="center_vertical|center"
        android:layout_alignTop="@+id/eleven"
        android:layout_toLeftOf="@+id/eleven"
        android:layout_alignBottom="@+id/eleven"
        android:layout_alignLeft="@+id/tv10"
        android:layout_alignStart="@+id/tv10"
        android:textColor="#01b29b" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView14"
        android:layout_toStartOf="@+id/one"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:textSize="20dp"
        android:layout_above="@+id/tv1"
        android:layout_below="@+id/spins" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Choose things appropriately and dont bother to fill boxes which isn&apos;t required."
        android:id="@+id/textView15"
        android:layout_below="@+id/spinb"
        android:layout_toLeftOf="@+id/textView14"
        android:layout_above="@+id/six"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="22dp"
        android:gravity="top|left"
        android:textColor="#ca969191" />

</RelativeLayout>
