        android:textSize="15dp"
        android:phoneNumber="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TH 2"
        android:id="@+id/tv2"
        android:layout_alignTop="@+id/two"
        android:layout_alignRight="@+id/tv1"
        android:layout_alignEnd="@+id/tv1"
        android:layout_alignBottom="@+id/two"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:gravity="center_vertical|center"
        android:textSize="20dp"
        android:textColor="#01b29b" />

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/three"
        android:layout_below="@+id/two"
        android:layout_alignLeft="@+id/two"
        android:layout_alignStart="@+id/two"
        android:layout_alignRight="@+id/two"
        android:layout_alignEnd="@+id/two"
        android:gravity="bottom|center"
        android:textSize="15dp"
        android:phoneNumber="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TH 3"
        android:id="@+id/tv3"
        android:layout_below="@+id/tv2"
        android:layout_alignRight="@+id/tv2"
        android:layout_alignEnd="@+id/tv2"
